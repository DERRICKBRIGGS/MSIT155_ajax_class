<h1>addApiimage</h1>
<img id="img1"/>

@* https://localhost:7159/Home/addApiimage *@
@section Scripts
{
    <script>
        const img1 = document.querySelector('#img1');
        (async() => {
            const url = '@Url.Content("~/api/returnimg/2")';
            const result = await fetch(url);
            const data = await result.blob();
            img1.src = URL.createObjectURL(data);
        })()
    </script>
}